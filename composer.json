{
    "name": "helsingborg-stad/modularity-like",
    "description": "Lets the users like a post.",
    "type": "wordpress-plugin",
    "license": "MIT",
    "scripts": {
        "test": "./vendor/bin/phpunit --testdox",
        "coverage": "XDEBUG_MODE=coverage ./vendor/bin/phpunit --testdox",
        "minimal": "./vendor/bin/phpunit"
    },
    "authors": [
        {
            "name": "Niclas Norin",
            "email": "niclas.norin@frontwalker.se"
        }
    ],
    "autoload": {
        "psr-4": {
            "ModularityLikePosts\\": "source/php/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "helsingborg-stad/municipio": "^6.0.0"
    },
    "require-dev": {
        "brain/monkey": "^2.6",
        "codedungeon/phpunit-result-printer": "^0.31.0",
        "phpunit/phpunit": "^9.5"
    },
    "extra": {
        "installer-paths": {
            "vendor/{$vendor}/{$name}": [
                "type:wordpress-muplugin",
                "type:wordpress-theme",
                "type:wordpress-plugin",
                "type:acf-plugin"
            ]
        }
    },
    "version": "3.0.4",
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
